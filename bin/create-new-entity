#!/usr/bin/env ./bin/php

<?php

include_once __DIR__ . '/../vendor/autoload.php';

use Entity\Test;
use Entity\TestEmbeddable;

$entityManager = EntityManagerFactory::create();

$entity = new Test(new TestEmbeddable('test'));
$entityManager->persist($entity);

$entityManager->flush();

echo 'Created Test entity with ID '.$entity->id.'.'.PHP_EOL;
