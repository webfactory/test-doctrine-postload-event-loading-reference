#!/usr/bin/env ./bin/php

<?php

include_once __DIR__ . '/../vendor/autoload.php';

use Entity\Test;
use Webfactory\Bundle\DownloadBundle\Entity\EmbeddableDownload;
use Webfactory\Download\Download;

$entityManager = EntityManagerFactory::create();

$entity = new Test(EmbeddableDownload::createFrom(new Download(new \GuzzleHttp\Psr7\Stream(tmpfile()))));
$entityManager->persist($entity);

$entityManager->flush();

echo 'Created Test entity with ID '.$entity->id.'.'.PHP_EOL;
